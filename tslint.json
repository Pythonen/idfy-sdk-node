{
  "extends": "tslint-config-airbnb",
  "rules": {
    "variable-name": [
      true,
      "allow-leading-underscore"
    ],
    "semicolon": [
      true,
      "always",
      "ignore-bound-class-methods"
    ],
    "trailing-comma": [
      false
    ],
    "max-line-length": [
      true,
      {
        "limit": 120
      }
    ]
  }
}
